{
    "name": "scan_ts",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fullpage/react-fullpage": "^0.1.40",
        "@reduxjs/toolkit": "^1.9.7",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "babel": "^6.23.0",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-styled-components": "^2.1.4",
        "html-entities": "^2.4.0",
        "node-polyfill-webpack-plugin": "^2.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.0",
        "react-responsive": "^9.0.2",
        "react-router": "^6.11.2",
        "react-router-dom": "^6.11.2",
        "react-router-hash-link": "^2.4.3",
        "react-scripts": "^5.0.1",
        "react-slick": "^0.29.0",
        "redux": "^4.2.1",
        "slick-carousel": "^1.8.1",
        "styled-components": "^6.1.0",
        "styled-components.macro": "^1.0.0",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "xml-js": "^1.6.11"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "p": "prettier . --write",
        "lint": "eslint --fix --ext .js,.jsx ."
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "no-unused-vars": "warn",
            "import/order": [
                "warn",
                {
                    "newlines-between": "always",
                    "groups": [
                        "builtin",
                        "external",
                        "parent",
                        "sibling",
                        "index",
                        "internal",
                        "object",
                        "type"
                    ],
                    "pathGroups": [
                        {
                            "pattern": "*.css",
                            "patternOptions": {
                                "matchBase": true
                            },
                            "group": "sibling",
                            "position": "after"
                        },
                        {
                            "pattern": "{*.svg,*.png,*.jpg}",
                            "patternOptions": {
                                "matchBase": true
                            },
                            "group": "sibling",
                            "position": "after"
                        },
                        {
                            "pattern": "{react,react-dom/**}",
                            "group": "external",
                            "position": "before"
                        }
                    ]
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "@redux-devtools/extension": "^3.2.5",
        "@types/react": "^18.2.30",
        "@types/react-dom": "^18.2.14",
        "@types/react-redux": "^7.1.28",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-router-hash-link": "^2.4.8",
        "@types/react-slick": "^0.23.11",
        "@types/styled-components": "^5.1.29",
        "babel-loader": "^9.1.3",
        "editorconfig": "^2.0.0",
        "eslint-plugin-import": "^2.28.1",
        "prettier": "^3.0.3"
    }
}
